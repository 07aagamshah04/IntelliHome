{
    "version": 2,
    "builds": [
      {
        "src": "Front-end/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Front-end/dist"
        }
      },
      {
        "src": "Back-end/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Back-end/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Front-end/dist/$1"
      }
    ]
  }